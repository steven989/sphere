                    checkInButtonPressDownTimer = setTimeout(function(){
                        checkInButtonPressDownTimer = null;
                        _this.removeClass('pressed');
                        _this.tooltip('destroy');
                        transition($(_this.data('current')),$(_this.data('target')));
                    },1000);
                }
            }).on("mouseup touchend",function(){
                _this.removeClass('pressed');
                _this.tooltip('destroy');
                clearTimeout(checkInButtonPressDownTimer);
                if (checkInButtonPressDownTimer != null) {buttonAjax($(this),{activity_definition_id:$(this).data('activity-definition-id'),notes:$('#checkInNotes').val()});}
