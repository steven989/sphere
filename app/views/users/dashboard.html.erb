    // Generic listener for ajax-enabled buttons

    function ajaxButtonListener(element) {
        if (element == null) {
            $('.ajaxButton').each(function(){
                $(this).off("click").on("click",function(){
                    buttonAjax($(this),$(this).data('data-to-send'));
                });
            });
