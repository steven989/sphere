<div data-remodal-id="importModal" class="importModalOnboarding" style="height:450px;" data-default-css="([{'attribute':'height','value':'450px'}])">
    <div class="modalMask"></div>
    <div class="modalView" id="mainImportView">
        <div class="modalContentContainer">
            <div class="addConnectionPopupContent" >
                <div class="import" data-intro="You can also import select connections from your Google Contacts" data-step="4" data-position="bottom">
                    <div class="formElement button textDarkGray disableWhileProcessed " href="<%= import_connection_path %>" data-method="put" data-data-to-send="({'provider':'google'})" id="importContactsGoogle" > <%= image_tag 'google-logo-small', width: 20 %> Select from Google Contacts</div>
                    <span class="textDarkGray" style="font-size:11px;">We respect your privacy. Sphere uses secure encrypted connection and will not spam your contacts or share their information</span>
                </div>
                <br>
                <h2 style="width:100%;text-align:center;border-bottom:1px solid #E0E0E0;line-height:0.34em;margin:0px 0px 20px;"><span style="font-size:14px;color:#787878;background-color:#f0f0f0;padding:0 19px;background:#FFF;">Or enter manually</span></h2>
                <p class="textDarkGray" data-intro="This shows you the remaining number of connections you can still add to your Sphere" data-step="5" data-position="bottom">You can add <span class="reminingContactDisplay"><%= @remaining_number_of_connections %></span> more connections to your Sphere</p>
                <form action="<%= create_connection_path %>" method="POST" data-intro="Enter some basic information about your connection and add them to your Sphere. Once they're in, you can start tracking your interactions with them." data-step="3" data-position="top">
                    <div class="vex-custom-field-wrapper">
                        <input class="formElement halfRow left m-b-md inputData clearInputAfterUpdate" data-default-class="formElement m-b-xs inputData" name="name" type="text" placeholder="Name" required>
                        <input class="formElement m-b-xs inputData clearInputAfterUpdate" name="email" type="text" placeholder="Email" >
                        <div id="cropInCreateConnection" class='hidden default_hidden'></div>
                        <input name="photoUploaderInCreate" data-uploader-crop-var="cropVarInCreateConnection" class="inputData clearInputAfterUpdate hiddenUploader" id="photoUploaderInCreate" type="file" accept="image/*">
                        <label for="photoUploaderInCreate" class="textDarkGray m-b-xs formElement button clearInputAfterUpdate" data-default-value='<%= h fa_icon 'id-badge' %> Upload Photo'>
                            <%= fa_icon 'id-badge' %>
                            Upload Photo
                        </label>                        
                        <textarea class="formElement inputData clearInputAfterUpdate" name="notes" placeholder="Notes" rows="3" required></textarea>
                        <select name="tags" id="tokenizeInCreate" multiple="multiple" class="modalTokenBox inputData tokenized clearInputAfterUpdate" data-token-variable="tokenInCreate">
                        </select>
                    </div>
                </form>
            </div>
            <span class="textDarkGray" style="font-size:11px;">Adding to Sphere does <b>not</b> send out an invite to connect. Sphere is your private space to track your connections</span>
        </div>
        <div class="modalButtonContainer">
            <div class="modalBottomFullButton sphereBlueBackground hoverable  disableWhileProcessed submitButton" data-submit-form=".modalView#mainImportView form">
                <span><%= fa_icon 'plus' %>  ADD TO SPHERE</span>
            </div>
        </div>
    </div>

    <div class="modalView hidden" id="listSelect">
        <div class="modalContentContainer p-h-xxlg">
            <div id="importContacts">
                <div class="search" style="width:100%; height:30%;">
                    <div class="alert hidden default_hidden" id="connectionFormAlert" style="margin-top:1px;line-height:10px;position:absolute;top:0px;left:0px;"></div>
                    <input type="text" class="search formElement" placeholder="Search">
                    <%= check_box_tag 'mergeName',true,true, style:"display:inline-box;", class:"m-t-sm" %>
                    <label for="mergeName" class="textDarkGray m-t-sm">Merge contacts with the same name</label>
                    <p class="textDarkGray">You can add <span class="reminingContactDisplay"><%= @remaining_number_of_connections %></span> more connections to your Sphere</p>
                </div>
                <div class="result" style="width:100%; height:70%;">
                    <table id="importSelectionTable" class="table table-sm table-hover textDarkGray">
                        <thead class="thead-default">
                            <th style="width:10%;">Import?</th>
                            <th style="width:30%;">Name</th>
                            <th style="width:40%;">Email</th>
                            <th style="width:10%;">Phone Numbers</th>
                            <th style="width:10%;">Other Emails</th>
                        </thead>
                        <tbody class="list">
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="modalButtonContainer">
            <div class="modalBottomLeftButton sphereRedBackground hoverable transitionButton" data-current="[data-remodal-id=importModal] .modalView#listSelect" data-target="[data-remodal-id=importModal] .modalView#mainImportView"><%= fa_icon "arrow-left" %>  BACK</div>
            <div class="modalBottomRightButton sphereBlueBackground hoverable  disableWhileProcessed" href="<%= create_from_import_path %>" data-method="POST"><%= fa_icon "plus" %>  IMPORT <span class="hideOnMobile">TO SPHERE</span></div>
        </div>
    </div>


</div>

