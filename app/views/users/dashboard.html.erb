    // This function takes the JSON list of contacts and populate it onto the popup
    function populateImportSelectionList(list) {
        $('.modalView#listSelect #importSelectionTable tbody').html("");
        var incrementer = 0;
        var contacts = list;
        returnedData,list = null; //this is to clear up the reference to the object to make it available for garbage collection after we're done
        contacts.forEach(function(contact){
            contact.incID = incrementer;
            $('.modalView#listSelect #importSelectionTable tbody').append('<tr data-inc-id='+incrementer+'><td class="import" ><input type="checkbox" class="importCheck" data-inc-id='+incrementer+' data-id="'+contact.id+'" data-name="'+contact.name+'" data-email="'+contact.email+'" data-phone="'+contact.phone.join('|>-<+|%')+'" data-other-emails="'+contact.other_emails.join('|>-<+|%')+'" ></td><td class="name" >'+contact.name+'</td><td class="email">'+contact.email+'</td><td class="phone">'+contact.phone.join(', ')+'</td><td class="other_emails">'+contact.other_emails.join(', ')+'</td></tr>');
            incrementer++;
        });
        
        $('#importContacts .search .search').off("keyup").on("keyup",function(){
            var keyword = $(this).val().toLowerCase();
            var matchedItemsIDs = contacts.filter(function(c){ return ((c.name+" "+c.email+" "+c.phone.join(', ')+" "+c.other_emails.join(', ')).toLowerCase().indexOf(keyword) > -1 ) }).map(function(c){return c.incID });
            $('#importContacts table tbody tr').each(function(){
                if ( matchedItemsIDs.indexOf($(this).data('inc-id')) > -1 ) {
                    $(this).removeClass("hidden");
                } else {
                    $(this).addClass('hidden');
                }
            });

        });
        $('.modalView#listSelect .modalBottomRightButton').off("click").on('click',function(){
            var contactListToSend = [];
            $('#importContacts table tbody tr td.import input[type=checkbox]').each(function(){
                if ($(this).prop('checked')) {
                    contactListToSend.push({id:$(this).data('id'),name:$(this).data('name'),email:$(this).data('email'),phone:$(this).data('phone'),other_emails:$(this).data('other-emails') })
                }
            });
            buttonAjax($(this),{contactsToImport:contactListToSend,mergeName:$('#importContacts #mergeName').prop('checked')});
        });

        $('#importContacts #importSelectionTable .importCheck').off("click").on("click",function(event){
            event.stopPropagation();
        }).off('change').on('change',function(){
            checkIfAtImportLimit();
        });
 
    }

    // Onboarding
    function showOnboarding(section,steps) {
        $('#mask').show();
        $('.modalMask').show();
        $('.disableWhileProcessed, .transitionButton').addClass("disabled");
        modalInstance.settings.closeOnEscape = false;
        modalInstance.settings.closeOnOutsideClick = false;
        introJs(section).setOption("showBullets", false).setOption("exitOnOverlayClick",false).setOption("exitOnEsc",false).start().oncomplete(function(){
            if (steps == "1,2") {
                setTimeout(function(){
                    $("#welcomeMessageModalView").addClass('hidden');
                    $("#challengeToAddThreeConnections").removeClass('hidden');
                    initializeReModal('[data-remodal-id=notificationsModal]',"standardModal",0);
                    $('#closeChallengeToAddThreeConnections').on("click",function(){
                        setTimeout(function(){
                            $('#addConnection').trigger("click"); 
                          },600);
                    });
                  },600);

            } else if (steps == "3,4,5") {
                onBoarding345 = true;
            } else if (steps == "9,10") {
                onBoarding910 = true;
            } else if (steps == "11,12,13") {
                onBoarding111213 = true;
            }
            logFrontEndActions(steps,"onboarding");
            $('#mask').hide();
            $('.modalMask').hide();
            $('.disableWhileProcessed, .transitionButton').removeClass("disabled");
            modalInstance.settings.closeOnEscape = true;
            modalInstance.settings.closeOnOutsideClick = true;
        }).onexit(function(){
            $('#mask').hide();
            $('.modalMask').hide();
            $('.disableWhileProcessed, .transitionButton').removeClass("disabled");
            modalInstance.settings.closeOnEscape = true;
            modalInstance.settings.closeOnOutsideClick = true;
        }).onafterchange(function(){
            if ($('a.introjs-skipbutton').html() == "Skip" || $('a.introjs-skipbutton').html() == "skip") {
                $('a.introjs-skipbutton').hide();
            } else if ($('a.introjs-skipbutton').html() == "Done" || $('a.introjs-skipbutton').html() == "done") {
                $('a.introjs-skipbutton').html("Close");
                $('a.introjs-skipbutton').show();
            }
        });
        $('a.introjs-skipbutton').hide();
        $('.introjs-showElement').removeClass('introjs-showElement');
        // $('a.introjs-button.introjs-skipbutton').remove();
    }
    
    <% if @settings[:onboarding_progress] && (!@settings[:onboarding_progress][1] || !@settings[:onboarding_progress][2]) %>
